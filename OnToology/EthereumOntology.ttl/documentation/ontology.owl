        <rdfs:label>logs bloom</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/Ethereum#/maxFeePerGas -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/Ethereum#/maxFeePerGas">
