        <rdfs:label>max fee per gas</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/Ethereum#/maxPriorityFeePerGas -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/Ethereum#/maxPriorityFeePerGas">
