        <rdfs:label>key</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/Ethereum#/logsBloom -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/Ethereum#/logsBloom">
