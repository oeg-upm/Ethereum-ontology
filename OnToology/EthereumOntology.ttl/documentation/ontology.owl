        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <dc:description>Cryptographic signature part of the transaction</dc:description>
        <rdfs:label>s</rdfs:label>
    </owl:DatatypeProperty>
    


